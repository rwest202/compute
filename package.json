{
    "name": "svelte-app",
    "version": "1.0.0",
    "scripts": {
        "build": "rollup -c",
        "dev": "routify -c dev-build",
        "dev-build": "rollup -c -w",
        "worker": "rollup -i src/workers/spiral.js -o public/spiral.js -p @rollup/plugin-json -w",
        "start": "sirv public -s"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "13.0.0",
        "@rollup/plugin-node-resolve": "8.0.1",
        "rollup": "2.15.0",
        "rollup-plugin-livereload": "1.3.0",
        "rollup-plugin-svelte": "5.2.2",
        "rollup-plugin-terser": "6.1.0",
        "svelte": "3.23.0"
    },
    "dependencies": {
        "@rollup/plugin-json": "4.1.0",
        "@simonwep/pickr": "1.7.2",
        "@sveltech/routify": "1.7.13",
        "lodash.debounce": "4.0.8",
        "paper": "0.12.8",
        "sirv-cli": "0.4.6"
    }
}
